            {
              "id": 1038752,
              "key": "8231274b-2dbb-46e7-9ea1-ca6f31ead817",
              "title": "ND9991 C03 L0 Stakeholders",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "OltKfmDMXds",
